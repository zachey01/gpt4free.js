{
  "name": "gpt4js",
  "version": "1.5.31",
  "description": "Using ChatGPT4/3.5-turbo/Gemini-Pro/BlackBox and etc. unlimited and free.",
  "main": "index.cjs",
  "module": "index.js",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "3.3.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "npx webpack --mode=production",
    "dev": "npx webpack serve",
    "build:bun": "bun build index.js --minify --outdir ./dist --target browser",
    "dev:bun": "bun build index.js --minify --outdir ./dist --target browser --watch",
    "test": "npx jest --config jest.config.js",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,md,mdx,css,yaml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachey01/gpt4free.js.git"
  },
  "author": "Zachey",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/zachey01/gpt4free.js/issues"
  },
  "bin": {
    "gpt4js": "./src/bin/main.js"
  },
  "homepage": "https://github.com/zachey01/gpt4free.js#readme",
  "keywords": [
    "nodejs",
    "bun",
    "gpt-3",
    "gpt-4",
    "chatgpt",
    "gpt4free",
    "ollama",
    "ollama-api",
    "gemini",
    "blackbox",
    "javascript",
    "ai",
    "chatbot",
    "nlp",
    "machine-learning"
  ]
}
